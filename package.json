{
  "name": "cupdate",
  "packageManager": "yarn@4.5.3",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "dev": "vite",
    "test": "vitest run --coverage.enabled --coverage.all --coverage.reportOnFailure --coverage.reportsDirectory \"$PWD/coverage\" --coverage.reporter html",
    "lint": "eslint --cache && prettier --check \"**/*.{ts,tsx,js,json,yml,yaml,mjs,md}\"",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,yml,yaml,mjs,md}\""
  },
  "dependencies": {
    "@xyflow/react": "12.3.6",
    "dompurify": "3.2.3",
    "elkjs": "0.9.3",
    "github-markdown-css": "5.8.1",
    "marked": "15.0.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "7.0.2"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.4",
    "@eslint/js": "9.16.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.0",
    "@types/dompurify": "3.2.0",
    "@types/eslint": "9.6.1",
    "@types/node": "22.10.2",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/parser": "8.18.0",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/coverage-v8": "2.1.8",
    "autoprefixer": "10.4.20",
    "eslint": "9.16.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-json": "4.0.1",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "15.13.0",
    "postcss": "8.4.49",
    "prettier": "3.4.2",
    "tailwindcss": "3.4.16",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.0",
    "vite": "6.0.3",
    "vitest": "2.1.8"
  }
}
